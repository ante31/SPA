#include <stdio.h>
#include <stdlib.h>
#include <time.h>

#define min(a,b) (((a)<(b))?(a):(b))

int* intersect(int* niz, int* niz2, int n, int m) {
	int* niz3 = (int*)malloc(sizeof(int) * min(n, m));
	int k = 0;
	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < m; j++)
		{
			if (niz[i] == niz2[j])
			{
				niz[k] = niz[i];
				k++;
			}
		}
	}
	return niz3;
}

void shuffle(int* niz, int n) {
	for (int i = 0; i < n; i++) {
		int j = (rand() * rand()) % n;
		int tmp = niz[i];
		niz[i] = niz[j];
		niz[j] = tmp;
	}
}

int* generiraj(int n) {
	int* niz = (int*)malloc(sizeof(int) * n);
	niz[0] = rand() % 5;
	for (int i = 1; i < n; i++) {
		niz[i] = niz[i - 1] + 1 + rand() % 5;
	}
	return niz;
}

void printout(int* niz, int n) {
	for (int i = 0; i < n; i++) {
		printf("%d ", niz[i]);
	}
	printf("\n");
}

void free_memory(int* niz) {
	free(niz);
}

int main()
{
	clock_t start_t, end_t;
	int n = 3000, m = 3000;
	srand(time(NULL));
	int* niz = generiraj(n);
	int* niz2 = generiraj(m);
	shuffle(niz, n);
	shuffle(niz2, m);
	start_t = clock();
	int* niz3 = intersect(niz, niz2, n, m);
	free_memory(niz3);
	end_t = clock();
	printf("Total time taken by CPU: %f\n", (double)(end_t - start_t) / CLOCKS_PER_SEC);
}
