#include <stdio.h>
#include <stdlib.h>

void intersect(int* niz, int* niz2, int n) {

	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < 4; j++)
		{
			if (niz[i] == niz2[j])
			{
				printf("%d", niz[i]);
			}
		}
	}
}

void shuffle(int* niz, int n) {
	for (int i = 0; i < n; i++) {
		int j = (rand() * rand()) % n;
		int tmp = niz[i];
		niz[i] = niz[j];
		niz[j] = tmp;
	}
}

int* generiraj(int n) {
	int* niz = (int*)malloc(sizeof(int) * n);
	niz[0] = rand() % 5;
	for (int i = 1; i < n; i++) {
		niz[i] = niz[i - 1] + 1 + rand() % 5;
	}
	return niz;
}

void printout(int* niz, int n) {
	for (int i = 0; i < n; i++) {
		printf("%d ", niz[i]);
	}
	printf("\n");
}

int main()
{
	int n = 5;
	srand(time(NULL));
	int* niz = generiraj(n);
	int* niz2 = generiraj(n);
	shuffle(niz, n);
  printout(niz, n);
	shuffle(niz2, n);
  printout(niz2, n);
	intersect(niz, niz2, n);
}
