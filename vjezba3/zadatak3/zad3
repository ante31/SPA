#include <stdio.h>
#include <stdlib.h>

int cmpfunc(const void* a, const void* b) {
	return (*(int*)a - *(int*)b);
}

void presjekPoredanihNizova(int* niz, int* niz2, int sizeofNiz) {

	int i = 0, j = 0;
	while (i < sizeofNiz) {
		if (niz[i] == niz2[j]) {
			printf("%d %d %d\n", niz[i], i, j);
			i++;
			j++;
		}
		else if (niz[i] < niz2[j]) {
			i++;
		}
		else {
			j++;
		}
	}
}





int main()
{
	int niz[] = { 2, 3, 12, 23 };
	int niz2[] = {1, 3, 7, 9, 12, 14, 16, 23};
	presjekPoredanihNizova(niz, niz2, sizeof(niz)/sizeof(niz[0]));
}

